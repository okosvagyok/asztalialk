<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="1280">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <Canvas>
            <Ellipse Fill="Aquamarine" Canvas.Left="520" Canvas.Top="10" Width="200" Height="200" Stroke="Beige" StrokeThickness="5" Panel.ZIndex="0"></Ellipse>
            <Rectangle Fill="AliceBlue" Canvas.Left="590" Canvas.Top="210" Width="50" Height="75" Stroke="Coral" Panel.ZIndex="0"></Rectangle>
            <Rectangle Fill="CadetBlue" Canvas.Left="520" Canvas.Top="285" Width="200" Height="250" Panel.ZIndex="0"></Rectangle>
            <Rectangle Fill="Chocolate" Canvas.Left="520" Canvas.Top="535" Width="60" Height="150" Panel.ZIndex="0"></Rectangle>
            <Rectangle Fill="Chocolate" Canvas.Left="660" Canvas.Top="535" Width="60" Height="150" Panel.ZIndex="0"></Rectangle>
            <Rectangle Fill="DodgerBlue" Canvas.Left="371" Canvas.Top="285" Width="150" Height="60" Panel.ZIndex="0"></Rectangle>
            <Rectangle Fill="DodgerBlue" Canvas.Left="720" Canvas.Top="285" Width="150" Height="60" Panel.ZIndex="0"></Rectangle>
            <Ellipse Fill="Black" Canvas.Left="570" Canvas.Top="75" Width="10" Height="10" Panel.ZIndex="1"></Ellipse>
            <Ellipse Fill="Black" Canvas.Left="650" Canvas.Top="75" Width="10" Height="10" Panel.ZIndex="1"></Ellipse>
            <Rectangle Fill="Black" Canvas.Left="580" Canvas.Top="140" Width="75" Height="5" Panel.ZIndex="1"></Rectangle>
        </Canvas>
    </Grid>
</Window>
